generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Session {
  id            String    @id
  shop          String
  state         String
  isOnline      Boolean   @default(false)
  scope         String?
  expires       DateTime?
  accessToken   String
  userId        BigInt?
  firstName     String?
  lastName      String?
  email         String?
  accountOwner  Boolean   @default(false)
  locale        String?
  collaborator  Boolean?  @default(false)
  emailVerified Boolean?  @default(false)
}

model tblkp_RepairEntryConditionReceived {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_condition_received")
}

model tblkp_RepairEntryPhotos {
  id    Int     @id @default(autoincrement())
  value Boolean

  @@map("repair_entry_photos")
}

model tblkp_RepairEntryDiagnosis {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_diagnosis")
}

model tblkp_RepairEntryDisposition {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_disposition")
}

model tblkp_RepairEntryReplacedItems {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_replaced_items")
}

model tblkp_RepairEntryFeedbackToCsd {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_feedback_to_csd")
}

model tblkp_RepairEntryReturnsRepairStatus {
  id    Int    @id @default(autoincrement())
  value String

  @@map("repair_entry_returns_repair_status")
}

model tblkp_RepairEntryReplacementItemDeliveryDate {
  id    Int      @id @default(autoincrement())
  value DateTime @db.Timestamptz(6)

  @@map("repair_entry_replacement_item_delivery_date")
}

model tblkp_CsdPrimaryCustomerReportedReasonForReturnWarranty {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_primary_customer_reported_reason_for_return_warranty")
}

model tblkp_CsdItem {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_item")
}

model tblkp_CsdReturnType {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_return_type")
}

model tblkp_CsdTroubleshootingNotes {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_troubleshooting_notes")
}

model tblkp_CsdCustomerServiceStatus {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_customer_service_status")
}

model tblkp_CsdRslCsd {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_rsl_csd")
}

model tblkp_CsdReturnItemRequired {
  id    Int     @id @default(autoincrement())
  value Boolean

  @@map("csd_return_item_required")
}

model tblkp_CsdRefundEligible {
  id    Int    @id @default(autoincrement())
  value String

  @@map("csd_refund_eligible")
}

model CsdEntry {
  id                                             Int       @id @default(autoincrement())
  serviceNumber                                  DateTime  @map("service_number") @db.Timestamptz(6)
  dateOfReturnRequest                            DateTime? @map("date_of_return_request") @db.Timestamptz(6)
  originalOrder                                  String?   @map("original_order")
  customerName                                   String?   @map("customer_name")
  primaryCustomerReportedReasonForReturnWarranty String?   @map("primary_customer_reported_reason_for_return_warranty")
  colUnnamed                                     String?   @map("col_unnamed")
  item                                           String?   @map("item")
  replacementOrder                               String?   @map("replacement_order")
  returnType                                     String?   @map("return_type")
  troubleshootingNotes                           String?   @map("troubleshooting_notes")
  customerServiceStatus                          String?   @map("customer_service_status")
  rslCsd                                         String?   @map("rsl_csd")
  returnItemRequired                             Boolean?  @map("return_item_required")
  repairDeptDesignation                          String?   @map("repair_dept_designation")
  createdAt                                      DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                                      DateTime  @default(now()) @map("updated_at") @db.Timestamptz(6)

  @@map("csd_entry")
}

model RepairEntry {
  id                    Int       @id @default(autoincrement())
  serviceNumber         DateTime? @map("service_number") @db.Timestamptz(6)
  dateOfRepairRequest   DateTime? @map("date_of_repair_request") @db.Timestamptz(6)
  originalOrder         String?   @map("original_order")
  customerName          String?   @map("customer_name")
  item                  String?   @map("item")
  replacementOrder      String?   @map("replacement_order")
  repairType            String?   @map("repair_type")
  repairNotes           String?   @map("repair_notes")
  customerServiceStatus String?   @map("customer_service_status")
  rslRepairDept         String?   @map("rsl_repair_dept")
  repairCompleted       Boolean?  @map("repair_completed")
  repairDeptDesignation String?   @map("repair_dept_designation")
  createdAt             DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt             DateTime  @default(now()) @map("updated_at") @db.Timestamptz(6)

  @@map("repair_entry")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model department {
  id   Int     @default(autoincrement())
  name String? @db.Char(1)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model note {
  id                Int     @default(autoincrement())
  department_id     Int?
  user_gid          String? @db.Char(1)
  note              String?
  return_entries_id Int?

  @@ignore
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model return_entries {
  id                           Int       @id(map: "return_entries_pk") @default(autoincrement())
  date_requested               DateTime? @db.Date
  original_order_id            String    @db.Char(1)
  original_order_gid           String?   @db.Char(1)
  customer_name                String    @db.Char(1)
  customer_gid                 String?   @db.Char(1)
  primary_customer_reason      Int?
  item                         Int?
  return_type                  Int?
  customer_service_status      Int?
  rsl_csr                      String?   @db.Char(1)
  rsl_csr_gid                  String?   @db.Char(1)
  return_item_required         Boolean?
  final_disposition            Int?
  date_received                DateTime? @db.Date
  repair_condition_received_id Int?
  date_inspected               DateTime? @db.Date
  rsl_rd_staff                 String?   @db.Char(1)
  rsl_rd_staff_gid             Int?
  replaced_items_todo          Int?
  status                       Int?
}
